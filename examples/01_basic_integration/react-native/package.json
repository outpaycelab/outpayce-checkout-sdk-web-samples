{
  "name": "OutpayceSDKTest",
  "version": "1.0.0",
  "description": "React Native sample app for Outpayce Checkout SDK - iOS 16 compatibility testing",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "ios:device": "react-native run-ios --device",
    "android": "react-native run-android",
    "lint": "eslint .",
    "test": "jest",
    "pod-install": "cd ios && pod install && cd ..",
    "clean": "watchman watch-del-all && rm -rf node_modules && rm -rf ios/Pods && rm -rf ios/build && npm install && cd ios && pod install && cd .."
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-webview": "^13.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "12.3.2",
    "@react-native-community/cli-platform-ios": "12.3.2",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
